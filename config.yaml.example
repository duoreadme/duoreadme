# DuoReadme 配置文件示例
# 复制此文件为 config.yaml 并填入您的配置

# 应用配置
app:
  bot_app_key: "your_bot_app_key_here"
  visitor_biz_id: "your_visitor_biz_id_here"

# 腾讯云配置
tencent_cloud:
  secret_id: "your_secret_id_here"
  secret_key: "your_secret_key_here"
  region: "ap-beijing"
  service: "lke"
  api_version: "2023-11-30"

# 翻译配置
translation:
  default_languages:
    - "zh"
    - "en"
    - "ja"
    # - "ko"
    # - "fr"
    # - "de"
    # - "es"
    # - "it"
    # - "pt"
    # - "ru"
  batch_size: 5
  timeout: 30



# SSE配置
sse:
  streaming_throttle: 1
  timeout: 60 